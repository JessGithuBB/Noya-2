<!DOCTYPE html>

<title>SsCategory</title>

{% block body %}
    <h1>SsCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ss_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ss_category.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ ss_category.slug }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ ss_category.description }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ ss_category.createdAt ? ss_category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ ss_category.updatedAt ? ss_category.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ss_category_index') }}">back to list</a>

    <a href="{{ path('app_ss_category_edit', {'id': ss_category.id}) }}">edit</a>

    {{ include('ss_category/_delete_form.html.twig') }}
{% endblock %}
